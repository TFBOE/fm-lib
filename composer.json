{
  "name": "tfboe/fm-lib",
  "description": "A lumen package with base functionality for all foosball manager applications",
  "type": "library",
  "keywords": [
    "foosball",
    "manager"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Benedikt Klocker",
      "email": "blacktemplar@a1.net"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/blacktemplar/jwt-auth-guard"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "gedmo/doctrine-extensions": "^2.4",
    "irazasyed/jwt-auth-guard": "^1.0.5",
    "laravel-doctrine/extensions": "^1.1",
    "laravel-doctrine/orm": "^1.5",
    "laravel/lumen-framework": "^6.0",
    "tymon/jwt-auth": "^1.0",
    "monolog/monolog": "^2.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.5",
    "fzaninotto/faker": "^1.8",
    "mockery/mockery": "^1.2",
    "phpmd/phpmd": "^2.6",
    "phpunit/phpcov": "^6.0",
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "psr-4": {
      "Tfboe\\FmLib\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tfboe\\FmLib\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
